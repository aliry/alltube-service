{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "jsx": true,
    "useJSXTextNode": true,
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "globals": {
    "NodeJS": true
  },
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "plugins": ["@typescript-eslint"],
  "settings": {}
}
