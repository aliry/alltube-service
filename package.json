{
  "name": "alltube-service",
  "version": "1.0.0",
  "description": "AllTube Api Service",
  "main": "dist/server.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "postinstall": "chmod +x ./scripts/dev.sh",
    "setup-actions": "chmod +x ./scripts/setup-github-actions.sh; ./scripts/setup-github-actions.sh",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "./scripts/dev.sh",
    "dev-server": "NODE_ENV=development nodemon --inspect=0.0.0.0:9229 ./dist/src/server.js",
    "ts-node-server": "ts-node src/server.ts",
    "start": "node dist/src/server.js",
    "test": "jest --forceExit",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d --force-recreate",
    "docker:prod": "docker-compose -f docker-compose.yml up -d --force-recreate"
  },
  "keywords": [],
  "author": "Ali Roudaki",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "get-folder-size": "^2.0.1",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0",
    "youtube-dl-exec": "^2.1.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/get-folder-size": "^2.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "jest-express": "^1.12.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}